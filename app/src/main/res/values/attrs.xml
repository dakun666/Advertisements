<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DotArrayView">

        <attr name="lines" format="integer" />
        <attr name="columns" format="integer" />
        <attr name="dotColor" format="color|reference" />
        <attr name="dotDiam" format="dimension" />
